{
  "name": "hokarinmari.github.io",
  "version": "0.1.0",
  "description": "Hokarinmari's Landing Page",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hokarinmari/hokarinmari.github.io.git"
  },
  "keywords": [
    "Landing",
    "Page",
    "React",
    "gatsby"
  ],
  "author": "pewriebontal <pewriebontal@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hokarinmari/hokarinmari.github.io/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "bootstrap": "^4.5.0",
    "gatsby": "^3.4.1",
    "gatsby-image": "^3.4.0",
    "gatsby-plugin-google-analytics": "^3.4.0",
    "gatsby-plugin-html-comments": "^1.0.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-mdx": "^2.4.0",
    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sass": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.1",
    "gatsby-plugin-sitemap": "^4.0.0",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "nanoid": "^3.1.10",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^17.0.1",
    "react-github-btn": "^1.2.0",
    "react-helmet": "^6.1.0",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.7.16",
    "react-tilt": "^0.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^6.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
